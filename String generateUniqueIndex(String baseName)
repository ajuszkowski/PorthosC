String generateUniqueIndex(String baseName) {
    int i = 1;
    String name;
    do {
        name = baseName + (i++);
    } while (isUnique(name));
    reservedNames.add(name);
    return name;
}

private List<String> reservedNames = new LinkedList<>();
private boolean isUnique(String name) {
    return reservedNames.contains(name);
}